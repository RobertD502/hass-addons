{
  "name": "Hass.io Rest980 Add-on",
  "version": "latest",
  "slug": "rest980",
  "description": "Allows installing the Rest980 Docker Image in Hass.io",
  "arch": [
    "amd64"
  ],
  "url": "https://github.com/jeremywillans/hass-addons/rest980",
  "startup": "application",
  "boot": "auto",
  "legacy": true,
  "ports": {
    "3000/tcp": 3000
  },
  "map": [
    "config:rw"
  ],
  "options": {
    "BLID": "blid",
    "PASSWORD": "password",
    "ROBOT_IP": "robot_ip",
    "FIRMWARE_VERSION": "2"
  },
  "schema": {
    "BLID": "str",
    "PASSWORD": "str",
    "ROBOT_IP": "str",
    "FIRMWARE_VERSION": "int"
  },
  "image": "koalazak/rest980"
}
